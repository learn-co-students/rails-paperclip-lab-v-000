<%= form_for @song  do |f| %>
  <%= f.label :album_cover, "Album Cover:" %>
  <%= f.file_field :album_cover %><br>
  <%= f.label :title, "Title:" %>
  <%= f.text_field :title %><br>
  <%= f.label :artist_name, "Artist Name:" %>
  <%= f.text_field :artist_name %>
  <br />

  <%= f.submit %>
<% end %>
