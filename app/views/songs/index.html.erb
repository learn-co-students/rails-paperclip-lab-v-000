<ul>
<% @songs.each do |song| %>
  <% if(song.artist.nil?) %>
    <%= link_to "Edit Song to Add Artist", edit_song_path(song) %>
  <% else %>
    <%= image_tag @song.album_cover.url(:thumb) unless @song.nil? %>
  <% end %>
  <li><%= display_artist(song) %> - <%= link_to song.title, song_path(song) %></li>
<% end %>
</ul>
<br>
<%= link_to "Add New Song", new_song_path %>
