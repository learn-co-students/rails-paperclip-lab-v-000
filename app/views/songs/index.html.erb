<ul>
<% @songs.each do |song| %>
  <% if song.album_cover.url(:thumb) %>
    <%= image_tag song.album_cover.url(:thumb) %>
  <% else %>
    <%= image_tag '/thumbs/default-user-image.png' %>
  <% end  %>
  <li><%= display_artist(song) %> - <%= link_to song.title, song_path(song) %></li>
<% end %>
</ul>
<br>
<%= link_to "Add New Song", new_song_path %>